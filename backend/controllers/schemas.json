{
	"Invoice": {
		"fields": {
			"invoice_number": {
				"label": "Número de factura",
				"regex": "(?i)\\b(?:número de factura|factura no\\.?|n°|no\\.?)\\s*[:\\-]?\\s*([\\d\\-\\.\\s]+)\\b"
			},
			"provider_ruc": {
				"label": "RUC",
				"regex": "(?i)\\b(?:ruc|r\\.u\\.c\\.|ruc / ci)\\s*[:\\-]?\\s*(\\d{10,13})\\b"
			},
			"provider_name": {
				"label": "Nombre del proveedor",
				"regex": "(?i)\\b(?:nombre del proveedor|proveedor|razón social)\\s*[:\\-]?\\s*([^\\n]+)"
			},
			"issue_date": {
				"label": "Fecha de emisión",
				"regex": "(?i)\\b(?:fecha de emisión|fecha|emisión)\\s*[:\\-]?\\s*(\\d{2}[/-]\\d{2}[/-]\\d{4})\\b"
			},
			"total": {
				"label": "Total",
				"regex": "(?i)\\b(?:total)\\s*[:\\-]?\\s*\\$?\\s*(\\d+(?:,\\d{3})*(?:\\.\\d{2})?)\\s*\\$?\\b"
			},
			"subtotal": {
				"label": "Subtotal",
				"regex": "(?i)\\b(?:subtotal(?:\\s*\\d+%|)|subtotal\\s*neto)\\s*[:\\-]?\\s*\\$?\\s*(\\d+(?:,\\d{3})*(?:\\.\\d{2})?)\\s*\\$?\\b"
			},
			"iva": {
				"label": "IVA",
				"regex": "(?i)\\b(?:iva\\s*\\d+%|impuesto)\\s*[:\\-]?\\s*\\$?\\s*(\\d+(?:,\\d{3})*(?:\\.\\d{2})?)\\s*\\$?\\b"
			},
			"client_name": {
				"label": "Cliente",
				"regex": "(?i)\\b(?:cliente|nombre cliente|consumidor final|razón social / nombres)\\s*[:\\-]?\\s*([^\\n]+)"
			},
			"client_id": {
				"label": "ID del Cliente",
				"regex": "(?i)\\b(?:cédula|identificación|id cliente|ruc / ci)\\s*[:\\-]?\\s*(\\d+)"
			},
			"logo": {
				"label": "Logo",
				"type": "image"
			}
		}
	},
	"ServiceDeliveryRecord": {
		"fields": {
			"hes_number": {
				"label": "Número de HES",
				"regex": "(?i)\\b(?:número de hes|hes no\\.?|n°|no\\.?)\\s*[:\\-]?\\s*(record-\\d{3})\\b"
			},
			"empresa_receptora": {
				"label": "Empresa que recibe el servicio",
				"regex": "(?i)\\b(?:empresa que recibe el servicio|empresa receptora)\\s*[:\\-]?\\s*([^\\n]+)"
			},
			"servicio": {
				"label": "Servicio",
				"regex": "(?i)\\b(?:servicio)\\s*[:\\-]?\\s*([^\\n]+)"
			},
			"ubicacion": {
				"label": "Ubicación del servicio",
				"regex": "(?i)\\b(?:ubicación del servicio|ubicación)\\s*[:\\-]?\\s*([^\\n]+)"
			},
			"contrato": {
				"label": "Contrato N°",
				"regex": "(?i)\\b(?:contrato n°|contrato no\\.?|n°|no\\.?)\\s*[:\\-]?\\s*(contract-\\d{4})\\b"
			},
			"fecha_inicio": {
				"label": "Fecha de inicio",
				"regex": "(?i)\\b(?:fecha de inicio)\\s*[:\\-]?\\s*(\\d{2}/\\d{2}/\\d{4})\\b"
			},
			"fecha_termino": {
				"label": "Fecha de término",
				"regex": "(?i)\\b(?:fecha de término|fecha de termino)\\s*[:\\-]?\\s*(\\d{2}/\\d{2}/\\d{4})\\b"
			},
			"firmas": {
				"label": "Firmas",
				"type": "array",
				"structure": [
					{
						"nombre": {
							"label": "Nombre",
							"regex": "(?i)^[A-Za-zÁáÉéÍíÓóÚúÑñ\\s]+$"
						},
						"cargo": {
							"label": "Cargo",
							"regex": "(?i)^[A-Za-zÁáÉéÍíÓóÚúÑñ\\s]+$"
						},
						"tipo": {
							"label": "Tipo de firma",
							"regex": "(?i)^(Proveedor|Receptor)$"
						}
					}
				]
			}
		}
	},
	"Contract": {
		"fields": {
			"contrato_number": {
				"label": "Número de Contrato",
				"regex": "(?i)\\b(?:contrato n°|contrato no\\.?|n°|no\\.?)\\s*[:\\-]?\\s*(contract-\\d{4})\\b"
			},
			"empresa_contratante": {
				"label": "Empresa Contratante",
				"regex": "(?i)\\b(?:empresa contratante)\\s*[:\\-]?\\s*([^\\n]+)"
			},
			"empresa_contratada": {
				"label": "Empresa Contratada",
				"regex": "(?i)\\b(?:empresa contratada)\\s*[:\\-]?\\s*([^\\n]+)"
			},
			"servicio": {
				"label": "Servicio",
				"regex": "(?i)\\b(?:servicio)\\s*[:\\-]?\\s*([^\\n]+)"
			},
			"fecha_inicio": {
				"label": "Fecha de inicio",
				"regex": "(?i)\\b(?:fecha de inicio)\\s*[:\\-]?\\s*(\\d{2}/\\d{2}/\\d{4})\\b"
			},
			"fecha_termino": {
				"label": "Fecha de término",
				"regex": "(?i)\\b(?:fecha de término|fecha de termino)\\s*[:\\-]?\\s*(\\d{2}/\\d{2}/\\d{4})\\b"
			}
		}
	}
}
