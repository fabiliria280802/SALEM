{
	"Invoice": {
	  "fields": {
		"company_name": {
		  "label": "Nombre de la Empresa",
		  "regex": "(?i)\\b(?:nombre de la empresa|company name)\\s*[:\\-]?\\s*([^\\n]+)"
		},
		"company_address": {
		  "label": "Dirección de la Empresa",
		  "regex": "(?i)\\b(?:dirección de la empresa|company address)\\s*[:\\-]?\\s*([^\\n]+)"
		},
		"company_phone": {
        "label": "Teléfono de la Empresa",
        "regex": "(?i)\\b(?:teléfono|phone)\\s*[:\\-]?\\s*(\\+?\\d[\\d\\-\\s]+)"
      	},
		"company_email": {
		  "label": "Email de la Empresa",
		  "regex": "(?i)\\b(?:email)\\s*[:\\-]?\\s*([^\\s]+@[\\w\\.-]+)"
		},
		"company_tax_id": {
		  "label": "ID Fiscal",
		  "regex": "(?i)\\b(?:id fiscal|tax id)\\s*[:\\-]?\\s*([^\\n]+)"
		},
		"invoice_number": {
		  "label": "Número de Factura",
		  "regex": "(?i)\\b(?:factura n°|invoice no)\\s*[:\\-]?\\s*(\\d+)"
		},
		"invoice_date": {
		  "label": "Fecha de Factura",
		  "regex": "(?i)\\b(?:fecha|date)\\s*[:\\-]?\\s*(\\d{2}/\\d{2}/\\d{4})"
		},
		"payable_at": {
		  "label": "Fecha de Vencimiento",
		  "regex": "(?i)\\b(?:vencimiento|payable at)\\s*[:\\-]?\\s*(\\d{2}/\\d{2}/\\d{4})"
		},
		"order_number": {
		  "label": "Número de Orden",
		  "regex": "(?i)\\b(?:orden n°|order no)\\s*[:\\-]?\\s*(\\d+)"
		},
		"client_name": {
		  "label": "Nombre del Cliente",
		  "regex": "(?i)\\b(?:cliente|client name)\\s*[:\\-]?\\s*([^\\n]+)"
		},
		"client_ruc": {
		  "label": "RUC del Cliente",
		  "regex": "(?i)\\b(?:ruc)\\s*[:\\-]?\\s*(\\d+)"
		},
		"client_address": {
		  "label": "Dirección del Cliente",
		  "regex": "(?i)\\b(?:dirección|address)\\s*[:\\-]?\\s*([^\\n]+)"
		},
		"items": {
		  "label": "Artículos",
		  "regex": "(?i)\\b(?:código|code)\\s*[:\\-]?\\s*([^\\n]+)"
		},
		"subtotal": {
		  "label": "Subtotal",
		  "regex": "(?i)\\b(?:subtotal)\\s*[:\\-]?\\s*\\$?\\s*(\\d+(?:\\.\\d{2})?)"
		},
		"tax": {
		  "label": "Impuesto",
		  "regex": "(?i)\\b(?:impuesto|tax)\\s*[:\\-]?\\s*\\$?\\s*(\\d+(?:\\.\\d{2})?)"
		},
		"total_due": {
		  "label": "Total a Pagar",
		  "regex": "(?i)\\b(?:total)\\s*[:\\-]?\\s*\\$?\\s*(\\d+(?:\\.\\d{2})?)"
		}
	  }
	},
	"ServiceDeliveryRecord": {
		"fields": {
		  "delivery_title": {
			"label": "Título del Acta",
			"regex": "(?i)\\b(?:acta de recepción|delivery receipt)\\b"
		  },
		  "receiver_date": {
			"label": "Fecha de Recepción",
			"regex": "(?i)\\b(?:fecha|date)\\s*[:\\-]?\\s*(\\d{2}/\\d{2}/\\d{4})"
		  },
		  "receiving_company": {
			"label": "Empresa Receptora",
			"regex": "(?i)\\b(?:empresa|company)\\s*[:\\-]?\\s*([^\\n]+)"
		  },
		  "provider": {
			"label": "Proveedor",
			"regex": "(?i)\\b(?:proporcionados por|provided by)\\s*[:\\-]?\\s*([^\\n]+)"
		  },
		  "order_number": {
			"label": "Número de Orden",
			"regex": "(?i)\\b(?:pedido identificado con el número|order identified with number)\\s*[:\\-]?\\s*(\\d+)"
		  },
		  "invoice_number": {
			"label": "Número de Factura",
			"regex": "(?i)\\b(?:factura número|invoice number)\\s*[:\\-]?\\s*(\\d+)"
		  },
		  "hes_number": {
			"label": "Número de HES",
			"regex": "(?i)\\b(?:hes asociado con el número|hes associated with the number)\\s*[:\\-]?\\s*(\\d+)"
		  },
		  "total_value": {
			"label": "Valor Total",
			"regex": "(?i)\\b(?:valor total de|total value of)\\s*[:\\-]?\\s*USD\\s*(\\d+(?:\\.\\d{2})?)"
		  },
		  "end_date": {
			"label": "Fecha de Término",
			"regex": "(?i)\\b(?:finalizó el día|ended on)\\s*[:\\-]?\\s*(\\d{2}/\\d{2}/\\d{4})"
		  },
		  "employee_name": {
			"label": "Nombre del Empleado",
			"regex": "(?i)\\b(?:empleado|employee)\\s*[:\\-]?\\s*([^\\n]+)"
		  },
		  "employee_signature": {
			"label": "Firma del Empleado",
			"regex": "(?i)\\b(?:firma|signature)\\s*[:\\-]?\\s*([^\\n]+)"
		  },
		  "employee_position": {
			"label": "Cargo del Empleado",
			"regex": "(?i)\\b(?:cargo|position)\\s*[:\\-]?\\s*([^\\n]+)"
		  }
		}
	},
	"Contract": {
		"fields": {
			"contract_number": {
				"label": "Número de Contrato",
				"regex": "(?i)\\b(?:contrato n°|contrato no\\.?|n°|no\\.?)\\s*[:\\-]?\\s*(contract-\\d{4})\\b"
			},
			"contracting_company": {
				"label": "Empresa Contratante",
				"regex": "(?i)\\b(?:empresa contratante)\\s*[:\\-]?\\s*([^\\n]+)"
			},
			"contracted_company": {
				"label": "Empresa Contratada",
				"regex": "(?i)\\b(?:empresa contratada)\\s*[:\\-]?\\s*([^\\n]+)"
			},
			"service": {
				"label": "Servicio",
				"regex": "(?i)\\b(?:servicio)\\s*[:\\-]?\\s*([^\\n]+)"
			},
			"start_date": {
				"label": "Fecha de inicio",
				"regex": "(?i)\\b(?:fecha de inicio)\\s*[:\\-]?\\s*(\\d{2}/\\d{2}/\\d{4})\\b"
			},
			"end_date": {
				"label": "Fecha de término",
				"regex": "(?i)\\b(?:fecha de término|fecha de termino)\\s*[:\\-]?\\s*(\\d{2}/\\d{2}/\\d{4})\\b"
			}
		}
	}
}
