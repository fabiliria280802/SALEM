{
	"Invoice": {
		"fields": {
			"factura_number": {
				"label": "Número de Factura",
				"regex": "(?i)\\b(?:factura n°|factura no\\.?|n°|no\\.?)\\s*[:\\-]?\\s*(invoice-\\d{6})\\b"
			},
			"fecha_emision": {
				"label": "Fecha de Emisión",
				"regex": "(?i)\\b(?:fecha de emisión|fecha)\\s*[:\\-]?\\s*(\\d{2}/\\d{2}/\\d{4})\\b"
			},
			"empresa_emisora": {
				"label": "Empresa Emisora",
				"regex": "(?i)\\b(?:empresa emisora)\\s*[:\\-]?\\s*([^\\n]+)"
			},
			"empresa_receptora": {
				"label": "Empresa Receptora",
				"regex": "(?i)\\b(?:empresa receptora)\\s*[:\\-]?\\s*([^\\n]+)"
			},
			"servicio": {
				"label": "Servicio",
				"regex": "(?i)\\b(?:servicio|detalle)\\s*[:\\-]?\\s*([^\\n]+)"
			},
			"subtotal": {
				"label": "Subtotal",
				"regex": "(?i)\\b(?:subtotal)\\s*[:\\-]?\\s*\\$?\\s*(\\d+(?:\\.\\d{2})?)\\b"
			},
			"descuento": {
				"label": "Descuento",
				"regex": "(?i)\\b(?:descuento\\s*\\(?\\d+%\\)?|descuento)\\s*[:\\-]?\\s*\\$?\\s*(\\d+(?:\\.\\d{2})?)\\b"
			},
			"subtotal_con_descuento": {
				"label": "Subtotal con descuento",
				"regex": "(?i)\\b(?:subtotal con descuento)\\s*[:\\-]?\\s*\\$?\\s*(\\d+(?:\\.\\d{2})?)\\b"
			},
			"iva": {
				"label": "IVA (15%)",
				"regex": "(?i)\\b(?:iva\\s*\\(?15%\\)?|iva)\\s*[:\\-]?\\s*\\$?\\s*(\\d+(?:\\.\\d{2})?)\\b"
			},
			"total": {
				"label": "Total a pagar",
				"regex": "(?i)\\b(?:total a pagar|total)\\s*[:\\-]?\\s*\\$?\\s*(\\d+(?:\\.\\d{2})?)\\b"
			}
		}
	},
	"ServiceDeliveryRecord": {
		"fields": {
			"hes_number": {
				"label": "Número de HES",
				"regex": "(?i)\\b(?:número de hes|hes no\\.?|n°|no\\.?)\\s*[:\\-]?\\s*(record-\\d{3})\\b"
			},
			"empresa_receptora": {
				"label": "Empresa que recibe el servicio",
				"regex": "(?i)\\b(?:empresa que recibe el servicio|empresa receptora)\\s*[:\\-]?\\s*([^\\n]+)"
			},
			"servicio": {
				"label": "Servicio",
				"regex": "(?i)\\b(?:servicio)\\s*[:\\-]?\\s*([^\\n]+)"
			},
			"ubicacion": {
				"label": "Ubicación del servicio",
				"regex": "(?i)\\b(?:ubicación del servicio|ubicación)\\s*[:\\-]?\\s*([^\\n]+)"
			},
			"contrato": {
				"label": "Contrato N°",
				"regex": "(?i)\\b(?:contrato n°|contrato no\\.?|n°|no\\.?)\\s*[:\\-]?\\s*(contract-\\d{4})\\b"
			},
			"fecha_inicio": {
				"label": "Fecha de inicio",
				"regex": "(?i)\\b(?:fecha de inicio)\\s*[:\\-]?\\s*(\\d{2}/\\d{2}/\\d{4})\\b"
			},
			"fecha_termino": {
				"label": "Fecha de término",
				"regex": "(?i)\\b(?:fecha de término|fecha de termino)\\s*[:\\-]?\\s*(\\d{2}/\\d{2}/\\d{4})\\b"
			},
			"firmas": {
				"label": "Firmas",
				"type": "array",
				"structure": [
					{
						"nombre": {
							"label": "Nombre",
							"regex": "(?i)^[A-Za-zÁáÉéÍíÓóÚúÑñ\\s]+$"
						},
						"cargo": {
							"label": "Cargo",
							"regex": "(?i)^[A-Za-zÁáÉéÍíÓóÚúÑñ\\s]+$"
						},
						"tipo": {
							"label": "Tipo de firma",
							"regex": "(?i)^(Proveedor|Receptor)$"
						}
					}
				]
			}
		}
	},
	"Contract": {
		"fields": {
			"contrato_number": {
				"label": "Número de Contrato",
				"regex": "(?i)\\b(?:contrato n°|contrato no\\.?|n°|no\\.?)\\s*[:\\-]?\\s*(contract-\\d{4})\\b"
			},
			"empresa_contratante": {
				"label": "Empresa Contratante",
				"regex": "(?i)\\b(?:empresa contratante)\\s*[:\\-]?\\s*([^\\n]+)"
			},
			"empresa_contratada": {
				"label": "Empresa Contratada",
				"regex": "(?i)\\b(?:empresa contratada)\\s*[:\\-]?\\s*([^\\n]+)"
			},
			"servicio": {
				"label": "Servicio",
				"regex": "(?i)\\b(?:servicio)\\s*[:\\-]?\\s*([^\\n]+)"
			},
			"fecha_inicio": {
				"label": "Fecha de inicio",
				"regex": "(?i)\\b(?:fecha de inicio)\\s*[:\\-]?\\s*(\\d{2}/\\d{2}/\\d{4})\\b"
			},
			"fecha_termino": {
				"label": "Fecha de término",
				"regex": "(?i)\\b(?:fecha de término|fecha de termino)\\s*[:\\-]?\\s*(\\d{2}/\\d{2}/\\d{4})\\b"
			}
		}
	}
}
